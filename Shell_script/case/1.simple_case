#Description: Simple case statement to print output as per user's input

echo "Enter a number (1 to 3):"
read n
case $n in
1) echo “You entered one” ;;
2) echo “You entered two” ;;
3) echo “You entered three” ;;
*) echo "Invalid choice"
esac
